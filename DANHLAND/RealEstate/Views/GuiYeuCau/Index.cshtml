@{
    bool menu = false;
    bool detail = false;
    bool del = false;
    bool edit = false;
    bool chidinh = false;
    if (User.IsInRole("17=1") || User.IsInRole("0=0"))
    {
        detail = true;
    }

    if (User.IsInRole("20=1") || User.IsInRole("0=0"))
    {
        del = true;
    }

    if (User.IsInRole("18=1") || User.IsInRole("0=0"))
    {
        edit = true;
    }
    if (User.IsInRole("19=1") || User.IsInRole("0=0"))
    {
        chidinh = true;
    }

    if (del || edit || detail || chidinh)
    {
        menu = true;
    }
}
