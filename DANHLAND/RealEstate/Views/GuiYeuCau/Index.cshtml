                                //if (itemData.text) { itemElement.append('<span class="navi-icon mr-2"><i class="' + itemData.icon + '"></i></span><span class="font-weight-bold font-size-h5 text-dark-75 text-hover-primary">' + itemData.text + '</span>'); } else {
